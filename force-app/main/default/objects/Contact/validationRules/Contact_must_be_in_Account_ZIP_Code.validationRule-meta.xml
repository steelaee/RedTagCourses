<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_must_be_in_Account_ZIP_Code</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(AccountId)),(MailingPostalCode !=  Account.ShippingPostalCode))</errorConditionFormula>
    <errorDisplayField>AccountId</errorDisplayField>
    <errorMessage>Account Number is not numeric.</errorMessage>
</ValidationRule>
